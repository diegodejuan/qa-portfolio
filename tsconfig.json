{
  "compilerOptions": {
  "target": "ESNext",
  "module": "CommonJS",
  "moduleResolution": "Node",
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "strict": true,
  "skipLibCheck": true,
  "types": ["node"],
  "resolveJsonModule": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
